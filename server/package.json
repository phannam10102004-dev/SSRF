    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dns": "^0.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ip": "^1.1.8",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "react-router-dom": "^7.9.6",
    "socket.io": "^4.8.1",
    "url": "^0.11.3",
    "validator": "^13.11.0"
