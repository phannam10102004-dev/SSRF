{
  "name": "ssrf-demo-server",
  "version": "1.0.0",
  "description": "Backend server cho SSRF demo",
  "main": "index.js",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "export": "node ../export-mongodb.js"
